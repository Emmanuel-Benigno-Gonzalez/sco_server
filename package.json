{
  "name": "sco-server",
  "version": "1.0.0",
  "description": "server sco con Express y Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "postinstall": "npm run build",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:api": "nodemon --exec ts-node src/index.ts --api",
    "migrate": "npx sequelize-cli db:migrate --config config/config.js",
    "generate:migration": "npx sequelize-cli migration:generate --name nombre-migracion --config config/config.js --migrations-path src/database/migrations"
  },
  "author": "Emmanuel Benigno Gonzalez",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.10",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  }
}
